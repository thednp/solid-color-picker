{
  "name": "@thednp/solid-color-picker",
  "version": "0.0.1",
  "description": "Modern Color Picker for SolidJS",
  "keywords": [
    "solid",
    "solid-component",
    "color picker",
    "colorpicker"
  ],
  "homepage": "https://github.com/thednp/solid-color-picker#readme",
  "bugs": {
    "url": "https://github.com/thednp/solid-color-picker/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thednp/solid-color-picker.git"
  },
  "sideEffects": false,
  "type": "module",
  "source": "src/index.tsx",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite serve example",
    "build:example": "vite build example",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"example/**/*.{js,ts,json,css,tsx,jsx}\"",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "lint": "pnpm lint:types && pnpm lint:code",
    "lint:code": "eslint --max-warnings 0 src/**/*.{ts,tsx}",
    "lint:types": "tsc --noEmit",
    "fix:ts": "eslint src/**/*.{ts,tsx} --fix",
    "update-deps": "pnpm up -Li"
  },
  "license": "MIT",
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "concurrently": "^8.2.0",
    "esbuild": "^0.19.2",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.47.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "solid-js": "^1.7.11",
    "tsup": "^7.2.0",
    "tsup-preset-solid": "^2.0.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-solid": "^2.7.0",
    "vite-plugin-solid-svg": "^0.6.4"
  },
  "dependencies": {
    "@thednp/color": "^1.0.8",
    "@thednp/color-picker": "2.0.0-alpha9",
    "@thednp/event-listener": "^2.0.2",
    "@thednp/shorty": "2.0.0-alpha17",
    "prettier": "^3.0.2"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": ">=16",
    "pnpm": ">=8.6.0"
  },
  "browser": {},
  "typesVersions": {}
}
