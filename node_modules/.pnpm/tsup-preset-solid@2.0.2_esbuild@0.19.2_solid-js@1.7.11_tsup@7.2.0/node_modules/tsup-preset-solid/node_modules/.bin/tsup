#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/solid-color-picker/solid-color-picker/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules/tsup/dist/node_modules:/home/runner/work/solid-color-picker/solid-color-picker/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules/tsup/node_modules:/home/runner/work/solid-color-picker/solid-color-picker/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules:/home/runner/work/solid-color-picker/solid-color-picker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/solid-color-picker/solid-color-picker/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules/tsup/dist/node_modules:/home/runner/work/solid-color-picker/solid-color-picker/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules/tsup/node_modules:/home/runner/work/solid-color-picker/solid-color-picker/node_modules/.pnpm/tsup@7.2.0_typescript@5.1.6/node_modules:/home/runner/work/solid-color-picker/solid-color-picker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tsup@7.2.0_typescript@5.1.6/node_modules/tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../../../../../tsup@7.2.0_typescript@5.1.6/node_modules/tsup/dist/cli-default.js" "$@"
fi
